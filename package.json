{
  "name": "@m0tivus/frontend-resources",
  "description": "motivus REST resources abstraction, useful for usual CRUD requirements.",
  "version": "0.4.3",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf dist",
    "prepare-build": "npm run clean && NODE_ENV=production babel src/lib --out-dir dist --extensions .js --ignore 'src/lib/**/*.text.js'",
    "copy": "cp package.json dist/ && cp README.md dist/ && cp .npmrc dist/",
    "build": "npm run prepare-build && npm run copy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "dependencies": {
    "@m0tivus/frontend-resources": "^0.1.10",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "clsx": "^1.1.1",
    "date-fns": "^2.19.0",
    "eslint-plugin-react": "^7.22.0",
    "formik": "^2.2.6",
    "lodash": "^4.17.21",
    "notistack": "^1.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@material-ui/core": "4.x",
    "@material-ui/pickers": "3.x"
  }
}
